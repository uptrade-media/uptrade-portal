{
  "name": "@uptrademedia/site-kit",
  "version": "1.0.15",
  "description": "Complete client-side integration kit for Uptrade Portal - SEO, Analytics, Engage, Forms, Blog",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "uptrade-setup": "./dist/cli/index.js",
    "uptrade-register-sitemap": "./dist/seo/register-sitemap-cli.js",
    "site-kit": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
