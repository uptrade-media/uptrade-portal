      "import": "./dist/images/index.mjs",
      "require": "./dist/images/index.js",
      "types": "./dist/images/index.d.ts"
    },
    "./reputation": {
      "import": "./dist/reputation/index.mjs",
      "require": "./dist/reputation/index.js",
      "types": "./dist/reputation/index.d.ts"
    },
    "./llms": {
      "import": "./dist/llms/index.mjs",
      "require": "./dist/llms/index.js",
      "types": "./dist/llms/index.d.ts"
