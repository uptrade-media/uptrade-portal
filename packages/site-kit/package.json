  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^17.2.3",
    "glob": "^10.3.0",
    "inquirer": "^9.2.0",
    "open": "^10.0.0",
    "ora": "^8.0.0",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@babel/types": "^7.28.6",
    "@types/babel__traverse": "^7.20.0",
